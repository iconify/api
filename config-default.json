{
  "port": 3000,
  "env-port": true,
  "region": "",
  "env-region": true,
  "reload-secret": "",
  "custom-icon-dirs": [
    "{dir}/json"
  ],
  "serve-default-icons": true,
  "index-page": "https://simplesvg.com/",
  "cache": {
    "timeout": 604800,
    "min-refresh": 604800,
    "private": false
  },
  "cors": {
    "origins": "*",
    "timeout": 86400,
    "methods": "GET, OPTIONS",
    "headers": "Origin, X-Requested-With, Content-Type, Accept, Accept-Encoding"
  }
}